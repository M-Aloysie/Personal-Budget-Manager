{"ast":null,"code":"export const expenseReducer=(state,action)=>{switch(action.type){case'ADD_EXPENSE':return[...state,action.payload];case'EDIT_EXPENSE':return state.map(expense=>expense.id===action.payload.id?action.payload:expense);case'DELETE_EXPENSE':return state.filter(expense=>expense.id!==action.payload);default:return state;}};","map":{"version":3,"names":["expenseReducer","state","action","type","payload","map","expense","id","filter"],"sources":["D:/Personal-Budget-Manager/src/reducers/expenseReducer.js"],"sourcesContent":["export const expenseReducer = (state, action) => {\r\n    switch (action.type) {\r\n      case 'ADD_EXPENSE':\r\n        return [...state, action.payload];\r\n      case 'EDIT_EXPENSE':\r\n        return state.map((expense) => \r\n          expense.id === action.payload.id ? action.payload : expense\r\n        );\r\n      case 'DELETE_EXPENSE':\r\n        return state.filter((expense) => expense.id !== action.payload);\r\n      default:\r\n        return state;\r\n    }\r\n  };\r\n  "],"mappings":"AAAA,MAAO,MAAM,CAAAA,cAAc,CAAGA,CAACC,KAAK,CAAEC,MAAM,GAAK,CAC7C,OAAQA,MAAM,CAACC,IAAI,EACjB,IAAK,aAAa,CAChB,MAAO,CAAC,GAAGF,KAAK,CAAEC,MAAM,CAACE,OAAO,CAAC,CACnC,IAAK,cAAc,CACjB,MAAO,CAAAH,KAAK,CAACI,GAAG,CAAEC,OAAO,EACvBA,OAAO,CAACC,EAAE,GAAKL,MAAM,CAACE,OAAO,CAACG,EAAE,CAAGL,MAAM,CAACE,OAAO,CAAGE,OACtD,CAAC,CACH,IAAK,gBAAgB,CACnB,MAAO,CAAAL,KAAK,CAACO,MAAM,CAAEF,OAAO,EAAKA,OAAO,CAACC,EAAE,GAAKL,MAAM,CAACE,OAAO,CAAC,CACjE,QACE,MAAO,CAAAH,KAAK,CAChB,CACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}